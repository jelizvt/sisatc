package com.sat.sisat.caja.dto;

import java.io.Serializable;
import java.util.Date;

public class AgenciaUsuarioDTO implements Serializable {

	private static final long serialVersionUID = 2108212823529158197L;

	private int agenciaUsuarioId;
	private int usuarioId;
	private int agenciaId;
	private String tipoRol;
	private Date fechaInicio;
	private Date fechaFin;
	private String ipAsignada;
	private String estado;
	private Date fechaRegistro;
	private String terminal;
	private String nombreAgencia;
	private String nombreCortoAgencia;
	private boolean selected;

	public AgenciaUsuarioDTO() {
		super();
	}

	public int getAgenciaUsuarioId() {
		return agenciaUsuarioId;
	}

	public void setAgenciaUsuarioId(int agenciaUsuarioId) {
		this.agenciaUsuarioId = agenciaUsuarioId;
	}

	public int getUsuarioId() {
		return usuarioId;
	}

	public void setUsuarioId(int usuarioId) {
		this.usuarioId = usuarioId;
	}

	public int getAgenciaId() {
		return agenciaId;
	}

	public void setAgenciaId(int agenciaId) {
		this.agenciaId = agenciaId;
	}

	public String getTipoRol() {
		return tipoRol;
	}

	public void setTipoRol(String tipoRol) {
		this.tipoRol = tipoRol;
	}

	public Date getFechaInicio() {
		return fechaInicio;
	}

	public void setFechaInicio(Date fechaInicio) {
		this.fechaInicio = fechaInicio;
	}

	public Date getFechaFin() {
		return fechaFin;
	}

	public void setFechaFin(Date fechaFin) {
		this.fechaFin = fechaFin;
	}

	public String getIpAsignada() {
		return ipAsignada == null ? "*" : ipAsignada;
	}

	public void setIpAsignada(String ipAsignada) {
		this.ipAsignada = ipAsignada;
	}

	public String getEstado() {
		return estado;
	}

	public void setEstado(String estado) {
		this.estado = estado;
	}

	public Date getFechaRegistro() {
		return fechaRegistro;
	}

	public void setFechaRegistro(Date fechaRegistro) {
		this.fechaRegistro = fechaRegistro;
	}

	public String getTerminal() {
		return terminal;
	}

	public void setTerminal(String terminal) {
		this.terminal = terminal;
	}

	public String getNombreAgencia() {
		return nombreAgencia;
	}

	public void setNombreAgencia(String nombreAgencia) {
		this.nombreAgencia = nombreAgencia;
	}

	public String getNombreCortoAgencia() {
		return nombreCortoAgencia;
	}

	public void setNombreCortoAgencia(String nombreCortoAgencia) {
		this.nombreCortoAgencia = nombreCortoAgencia;
	}

	public boolean isSelected() {
		return selected;
	}

	public void setSelected(boolean selected) {
		this.selected = selected;
	}
}
