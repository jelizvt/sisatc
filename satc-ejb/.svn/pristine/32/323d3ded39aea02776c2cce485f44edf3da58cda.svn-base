package com.sat.sisat.predial.dto;

import java.io.Serializable;

public class SustentoCondicionEspecialDTO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private int condicionRequisitoId;
	private String descripcioRequsito;
	private String glosaRequisito;
	private int flag;
	private int presentado;
	
	public SustentoCondicionEspecialDTO() {
		super();
	}

	public SustentoCondicionEspecialDTO(int condicionRequisitoId, String descripcioRequsito,
			String glosaRequisito,int flag) {
		super();
		this.condicionRequisitoId = condicionRequisitoId;
		this.descripcioRequsito = descripcioRequsito;
		this.glosaRequisito = glosaRequisito;
		this.flag = flag;
	}
	
	public int getCondicionRequisitoId() {
		return condicionRequisitoId;
	}
	public void setCondicionRequisitoId(int condicionRequisitoId) {
		this.condicionRequisitoId = condicionRequisitoId;
	}
	public String getDescripcioRequsito() {
		return descripcioRequsito;
	}
	public void setDescripcioRequsito(String descripcioRequsito) {
		this.descripcioRequsito = descripcioRequsito;
	}
	public String getGlosaRequisito() {
		return glosaRequisito;
	}
	public void setGlosaRequisito(String glosaRequisito) {
		this.glosaRequisito = glosaRequisito;
	}

	public int getFlag() {
		return flag;
	}

	public void setFlag(int flag) {
		this.flag = flag;
	}

	public int getPresentado() {
		return presentado;
	}

	public void setPresentado(int presentado) {
		this.presentado = presentado;
	}
	
	

}
