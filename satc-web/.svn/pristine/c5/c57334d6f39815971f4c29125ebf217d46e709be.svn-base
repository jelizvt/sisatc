                   <rich:column width="10px">
	            	<a4j:commandLink reRender="panelConstruccion" ajaxSingle="true" id="editlinksucursal" action="#{registroPredioManaged.editarConstruccion}" oncomplete="#{rich:component('panelConstruccion')}.show()">
	                     <h:graphicImage value="/images/modal/edit.gif" style="border:0" />
	                     <f:setPropertyActionListener value="#{record}" target="#{registroPredioManaged.currentItem}" id="r42"/>
	                </a4j:commandLink>
	                
		        </rich:column>
       </rich:dataTable>
</h:panelGrid>


