	                    </rich:column>
	                    <rich:column rowspan="2">
	                    	<h:outputText value="Nro Nivel"/>
	                    </rich:column>
	                    <rich:column rowspan="2">
	                    	<h:outputText value="Area Construccion"/>
	                    </rich:column>
	                    <rich:column rowspan="2">
	                    	<h:outputText value="Area Comun Constr."/>
	                    </rich:column>
	                    <rich:column rowspan="2">
	                    	<h:outputText value="Material"/>
	                    </rich:column>
	                    <rich:column rowspan="2">
