package com.sat.sisat.exception;

public class SisatException extends Exception {

	private static final long serialVersionUID = 6750812435097116024L;

	public SisatException() {
		super();
	}

	public SisatException(String message, Throwable cause) {
		super(message, cause);
	}

	public SisatException(String message) {
		super(message);
	}

	public SisatException(Throwable cause) {
		super(cause);
	}
}
