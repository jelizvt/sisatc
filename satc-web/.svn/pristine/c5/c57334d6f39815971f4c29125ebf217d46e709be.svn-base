	                    	<h:outputText value="T"/>
	                    </rich:column>
	                    <rich:column>
	                    	<h:outputText value="P"/>
	                    </rich:column>
	                    <rich:column>
	                    	<h:outputText value="PV"/>
	                    </rich:column>
	                    <rich:column>
	                    	<h:outputText value="R"/>
	                    </rich:column>
	                    <rich:column>
	                    	<h:outputText value="B"/>
	                    </rich:column>
	                    <rich:column>
	                    	<h:outputText value="I"/>
	                    </rich:column>
	                     <rich:column>
	                    	<h:outputText value=""/>
	                    </rich:column>
		            </rich:columnGroup>
				</f:facet>
			  	<rich:column>
                   	<h:outputText value="#{record.item}"/>
                   </rich:column>
                   <rich:column >
                       <h:outputText value="#{record.dentiponivel}" />
                   </rich:column>
                   <rich:column >
                       <h:outputText value="#{record.nroNivel}"/>
                   </rich:column>
                   <rich:column >
                   	<h:outputText value="#{record.areaConstruccion}"/>
                   </rich:column>
                   <rich:column >
                   	<h:outputText value="#{record.areaComunConstruccion}"/>
                   </rich:column>
                   <rich:column >
