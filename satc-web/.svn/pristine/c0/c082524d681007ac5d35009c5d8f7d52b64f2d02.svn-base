<div align="center">
	<h:form>
		<input type="button" value="Imprimir" onclick="window.print(); javascript:window.close();" />
	</h:form>
	<br />
	<h:outputText value="REPORTE DE CUENTAS " styleClass="texto" />
	<br />
	<h:outputText value="del " />
	<h:outputText value="#{reporteCuentaManaged.fechaInicio}" style="font-weight: bold;">
		<f:convertDateTime pattern="#{msg['param.datepattern']}" />
	</h:outputText>
	<h:outputText value=" al " />
	<h:outputText value="#{reporteCuentaManaged.fechaFin}" style="font-weight: bold;">
		<f:convertDateTime pattern="#{msg['param.datepattern']}" />
	</h:outputText>
	<br />

<div style="width: 635px;">
<rich:panel >
	<f:facet name="header">
		<div align="center">SAT</div>
	</f:facet>
	<rich:dataTable width="600px" value="#{reporteCuentaManaged.listaCuentaDTOs}" rows="5" first="0" var="item">
		<rich:column width="320px">
			<f:facet name="header">
				<div align="center">Descripcion</div>
			</f:facet>
			<h:outputText value="#{item.descripcionCuenta}" />
		</rich:column>
		<rich:column width="190px">
			<f:facet name="header">
				<div align="center">Nro Cuenta Corriente</div>
			</f:facet>
		</rich:column>
		<rich:column width="110px">
			<f:facet name="header">
				<div align="center">Total</div>
			</f:facet>
			<div align="right">
				<h:outputText value="#{item.monto}">
					<f:convertNumber locale="#{msg['param.localecurrency']}" minFractionDigits="2" maxFractionDigits="2"/>
				</h:outputText>
			</div>
		</rich:column>
	</rich:dataTable>
	<h:panelGrid columns="2" columnClasses="col_result_forma1_1, col_result_forma1_3" cellpadding="0" cellspacing="0">
		<h:outputText value="Sub Total" />
		<h:outputText value="#{reporteCuentaManaged.listaCuentaDTOs.get(18).monto}" style="align:right">
			<f:convertNumber locale="#{msg['param.localecurrency']}" minFractionDigits="2" maxFractionDigits="2"/>
		</h:outputText>
	</h:panelGrid>
</rich:panel>
<rich:panel>
