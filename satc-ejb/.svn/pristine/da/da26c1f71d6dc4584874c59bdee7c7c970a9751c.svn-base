package com.sat.sisat.vehicular.dto;

import java.io.Serializable;

public class DocumentoSustentatorioDTO implements Serializable{

	private int docSustentatorioId;
	private String descripcion;
	private String glosa;
	private boolean selected;
	
	//INICIO IVO
	private String nomDocAdjunto;
	
	private Integer contentId;
	
	public String getNomDocAdjunto() {
		return nomDocAdjunto;
	}

	public void setNomDocAdjunto(String nomDocAdjunto) {
		this.nomDocAdjunto = nomDocAdjunto;
	}
	
	public Integer getContentId() {
		return contentId;
	}

	public void setContentId(Integer contentId) {
		this.contentId = contentId;
	}

	public DocumentoSustentatorioDTO(int docSustentatorioId,
			String descripcion, String glosa, boolean selected, String nomDocAdj, Integer contentId) {
		super();
		this.docSustentatorioId = docSustentatorioId;
		this.descripcion = descripcion;
		this.glosa = glosa;
		this.selected = selected;
		this.nomDocAdjunto=nomDocAdj;
		this.contentId=contentId;
	}
	//FIN IVO



	public DocumentoSustentatorioDTO() {
		super();
	}


	public int getDocSustentatorioId() {
		return docSustentatorioId;
	}

	public void setDocSustentatorioId(int docSustentatorioId) {
		this.docSustentatorioId = docSustentatorioId;
	}

	public String getDescripcion() {
		return descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

	public String getGlosa() {
		return glosa;
	}

	public void setGlosa(String glosa) {
		this.glosa = glosa;
	}

	public boolean isSelected() {
		return selected;
	}

	public void setSelected(boolean selected) {
		this.selected = selected;
	}


}
