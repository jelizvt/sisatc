package com.sat.sisat.predial.dto;

import java.io.Serializable;
import java.math.BigDecimal;


public class SeguridadArbitriosDTO implements Serializable {
	
	public String uso;
	public BigDecimal tasaAnual;
	public BigDecimal tasaAnualPreSubven;
	public BigDecimal tasaAnualAntesBeneficios;
	private Integer categoriaUsoId;

	public BigDecimal getTasaAnualPreSubven() {
		return tasaAnualPreSubven;
	}

	public void setTasaAnualPreSubven(BigDecimal tasaAnualPreSubven) {
		this.tasaAnualPreSubven = tasaAnualPreSubven;
	}
	
	public Integer getCategoriaUsoId() {
		return categoriaUsoId;
	}

	public void setCategoriaUsoId(Integer categoriaUsoId) {
		this.categoriaUsoId = categoriaUsoId;
	}

	public BigDecimal getTasaAnual() {
		return tasaAnual;
	}

	public void setTasaAnual(BigDecimal tasaAnual) {
		this.tasaAnual = tasaAnual;
	}

	public String getUso() {
		return uso;
	}

	public void setUso(String uso) {
		this.uso = uso;
	}

	public SeguridadArbitriosDTO(){
		
	}

	public BigDecimal getTasaAnualAntesBeneficios() {
		return tasaAnualAntesBeneficios;
	}

	public void setTasaAnualAntesBeneficios(BigDecimal tasaAnualAntesBeneficios) {
		this.tasaAnualAntesBeneficios = tasaAnualAntesBeneficios;
	}
}
