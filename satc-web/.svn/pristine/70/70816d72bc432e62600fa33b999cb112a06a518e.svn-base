							<td><rich:comboBox width="250px" defaultLabel="Seleccione"
									onblur="validaInputCbo(this)"
									value="#{buscarBeneficioTributarioManaged.cmbMotivo}"
									requiredMessage="#{msg['cc.errormotivonotificacion']}"
									required="true" id="id_formaNotificacion"
									binding="#{buscarBeneficioTributarioManaged.cmbxMotivo}"
									valueChangeListener="#{buscarBeneficioTributarioManaged.motivoNotificacionById}">
									<f:selectItems id="id_lsttiporelacion"
										value="#{buscarBeneficioTributarioManaged.listaMotivoNotificacion}" />
								</rich:comboBox></td>
						</tr>
						<tr>
							<td align="left" width="130px"><h:outputText
									value="Notificador:" /> <span style="color: red;">*</span></td>
							<td><h:panelGroup>
									<rich:comboBox width="250px"
										defaultLabel="Seleccione Notificador"
										onblur="validaInputCbo(this)"
										value="#{buscarBeneficioTributarioManaged.cmbNotifica}"
										requiredMessage="Seleccione Un Notificador']}" required="true"
										id="id_notificador"
										binding="#{buscarBeneficioTributarioManaged.cmbxNotificador}"
										valueChangeListener="#{buscarBeneficioTributarioManaged.notificadorById}">
										<f:selectItems id="id_lstNotificador"
											value="#{buscarBeneficioTributarioManaged.listaNotificador}" />
									</rich:comboBox>

								</h:panelGroup></td>

						</tr>
						<tr>
							<td align="left" width="130px"><h:outputText
									value="Fecha de Notificación" /> <span style="color: red;">*</span>
							</td>
							<td align="left" width="100px"><rich:calendar locale="en/US"
									required="true"
									requiredMessage="#{msg['cc.errorfechanotificacion']}"
									datePattern="dd/MM/yyyy HH:mm" direction="top-right"
									enableManualInput="true" inputClass="maskDate"
									inputStyle="width:100px" id="id_fechadeclaracion"
									value="#{buscarBeneficioTributarioManaged.fechaNotifica}"
									showApplyButton="true">
								</rich:calendar> <rich:jQuery id="maskedo3" selector=".maskDate"
									query="mask('99/99/9999 99:99')" timing="onload" /></td>
						</tr>

					</table>

				</a4j:outputPanel>
				<h:panelGrid columns="2">
					<a4j:commandButton value="Aceptar" style="width:100px;"
						action="#{buscarBeneficioTributarioManaged.notificaResolucion}"
						reRender="datosactos,panel"
						oncomplete="#{facesContext.maximumSeverity == null ? 'Richfaces.hideModalPanel(\'mpErrors\'); Richfaces.showModalPanel(\'panelNotificarResol\'); return false' : 'Richfaces.showModalPanel(\'mpErrors\');' };"
						disabled="#{buscarBeneficioTributarioManaged.tipoAccion == null}">
					</a4j:commandButton>
					<a4j:commandButton value="Salir" style="width:100px;"
						action="#{buscarBeneficioTributarioManaged.salir}"
						status="ajaxStatus"
						oncomplete="#{rich:component('panelNotificarResol')}.hide(); return false">
					</a4j:commandButton>
				</h:panelGrid>
			</h:panelGrid>
		</h:form>
	</rich:modalPanel>
