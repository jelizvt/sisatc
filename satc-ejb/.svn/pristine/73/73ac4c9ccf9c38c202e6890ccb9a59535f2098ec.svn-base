package com.sat.sisat.vehicular.dto;

import java.io.Serializable;
import java.util.Date;

public class RvTransferenteDTO implements Serializable {
	private int transferenciaId;
	private int djVehicularId;
	private int personaId;
	private String tipo;
	private Date fechaActualizacion;
	private int usuarioId;
	private Date fechaRegistro;
	private String estado;
	private String terminal;

	public RvTransferenteDTO() {
	}

	public RvTransferenteDTO(int djVehicularId, int personaId) {
		super();
		this.djVehicularId = djVehicularId;
		this.personaId = personaId;
	}

	public int getTransferenciaId() {
		return transferenciaId;
	}

	public int getDjVehicularId() {
		return djVehicularId;
	}

	public int getPersonaId() {
		return personaId;
	}

	public String getTipo() {
		return tipo;
	}

	public Date getFechaActualizacion() {
		return fechaActualizacion;
	}

	public int getUsuarioId() {
		return usuarioId;
	}

	public Date getFechaRegistro() {
		return fechaRegistro;
	}

	public String getEstado() {
		return estado;
	}

	public String getTerminal() {
		return terminal;
	}

	public void setTransferenciaId(int transferenciaId) {
		this.transferenciaId = transferenciaId;
	}

	public void setDjVehicularId(int djVehicularId) {
		this.djVehicularId = djVehicularId;
	}

	public void setPersonaId(int personaId) {
		this.personaId = personaId;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public void setFechaActualizacion(Date fechaActualizacion) {
		this.fechaActualizacion = fechaActualizacion;
	}

	public void setUsuarioId(int usuarioId) {
		this.usuarioId = usuarioId;
	}

	public void setFechaRegistro(Date fechaRegistro) {
		this.fechaRegistro = fechaRegistro;
	}

	public void setEstado(String estado) {
		this.estado = estado;
	}

	public void setTerminal(String terminal) {
		this.terminal = terminal;
	}

}
