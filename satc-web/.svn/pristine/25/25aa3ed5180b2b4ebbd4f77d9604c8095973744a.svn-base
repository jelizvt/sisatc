	public Boolean validaPapeleta(){
		if(listCtaCte!=null&&listCtaCte.size()>0){
			//se verifica que exista una seleccion
			int i=0;
			for (DeudaDTO de : listCtaCte) {
				if (de.isSelected()) {
					i++;
				}
			}	
			if (i>0){
