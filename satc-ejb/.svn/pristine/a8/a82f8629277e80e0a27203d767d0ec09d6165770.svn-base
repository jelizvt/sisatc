package com.sat.sisat.controlycobranza.dto;

import java.io.Serializable;
import java.sql.Timestamp;



public class FindPeriodoCuota  implements Serializable {
	
	private int vencimientoId;

	private int conceptoId;

	private int cuota;

	private String estado;

	private Timestamp fechaRegistro;

	private Timestamp fechaVencimiento;

	private int mesFin;

	private int mesInicio;

	private int periodo;

	private int subconceptoId;

	private String terminal;

	private int usuarioId;

    public FindPeriodoCuota() {
    }

	public int getVencimientoId() {
		return vencimientoId;
	}

	public void setVencimientoId(int vencimientoId) {
		this.vencimientoId = vencimientoId;
	}

	public int getConceptoId() {
		return conceptoId;
	}

	public void setConceptoId(int conceptoId) {
		this.conceptoId = conceptoId;
	}

	public int getCuota() {
		return cuota;
	}

	public void setCuota(int cuota) {
		this.cuota = cuota;
	}

	public String getEstado() {
		return estado;
	}

	public void setEstado(String estado) {
		this.estado = estado;
	}

	public Timestamp getFechaRegistro() {
		return fechaRegistro;
	}

	public void setFechaRegistro(Timestamp fechaRegistro) {
		this.fechaRegistro = fechaRegistro;
	}

	public Timestamp getFechaVencimiento() {
		return fechaVencimiento;
	}

	public void setFechaVencimiento(Timestamp fechaVencimiento) {
		this.fechaVencimiento = fechaVencimiento;
	}

	public int getMesFin() {
		return mesFin;
	}

	public void setMesFin(int mesFin) {
		this.mesFin = mesFin;
	}

	public int getMesInicio() {
		return mesInicio;
	}

	public void setMesInicio(int mesInicio) {
		this.mesInicio = mesInicio;
	}

	public int getPeriodo() {
		return periodo;
	}

	public void setPeriodo(int periodo) {
		this.periodo = periodo;
	}

	public int getSubconceptoId() {
		return subconceptoId;
	}

	public void setSubconceptoId(int subconceptoId) {
		this.subconceptoId = subconceptoId;
	}

	public String getTerminal() {
		return terminal;
	}

	public void setTerminal(String terminal) {
		this.terminal = terminal;
	}

	public int getUsuarioId() {
		return usuarioId;
	}

	public void setUsuarioId(int usuarioId) {
		this.usuarioId = usuarioId;
	}
	@Override
	public String toString(){
		return periodo+"-"+cuota;
	}
}
