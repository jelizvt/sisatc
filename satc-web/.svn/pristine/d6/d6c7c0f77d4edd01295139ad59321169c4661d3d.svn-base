textarea[type="textarea"][disabled],input[type="text"][disabled],input[type="password"][disabled],select[disabled]
	{
	color: #000000;
	border: none;
}

.tablaBorde {
	border-width: 1px;
	border-style: dotted;
	border-color: #cccccc;
	border-collapse: collapse;
}

.tablaBorde td {
	border-width: 1px;
	border-style: dotted;
	border-color: #cccccc;
}

.boldText {
	font-weight: bold;
}

input[type="button"].nueva_djvehicular {
	background-image: url(../images/nuevo_vehiculo.png);
	width: 48px;
	height: 27px;
	color: #FFF;
	cursor: pointer;
	background-color: transparent;
	font-family: inherit;
	border: 0px;
}

input[type="button"].verDatos {
	background-image: url(../images/ver2.png);
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
	width: 16px;
	height: 16px;
	cursor: pointer;
	background-color: transparent;
	border: 0px;
}

input[type="button"].actualizarDatos {
	background-image: url(../images/editar2.png);
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
	width: 17px;
	height: 16px;
	cursor: pointer;
	background-color: transparent;
	border: 0px;
}

input[type="button"][disabled].actualizarDatos {
	background-image: url(../images/editar2_ds.png);
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
	width: 17px;
	height: 16px;
	cursor: default;
	background-color: transparent;
	border: 0px;
}

input[type="button"].determinarDj {
	background-image: url(../images/calculo2.png);
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
	width: 11px;
	height: 16px;
	cursor: pointer;
	background-color: transparent;
	border: 0px;
}

input[type="button"][disabled].determinarDj {
	background-image: url(../images/calculo2_ds.png);
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
	width: 11px;
	height: 16px;
	cursor: default;
	background-color: transparent;
	border: 0px;
}

input[type="button"].copiarDjOtroAnio {
	background-image: url(../images/copiar_dj.png);
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
