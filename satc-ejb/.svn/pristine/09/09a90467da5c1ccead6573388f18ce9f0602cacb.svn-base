package com.sat.sisat.usuario.dto;
//-=CRAMIREZ=-
import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

public class MenusDTO implements Serializable{
	
	private static final long serialVersionUID = 4636686895544895555L;

	private int menuId;
	
	private String menu;
	
	private List<SubMenuDTO> listMenus = new ArrayList<SubMenuDTO>();

	public int getMenuId() {
		return menuId;
	}

	public void setMenuId(int menuId) {
		this.menuId = menuId;
	}

	public String getMenu() {
		return menu;
	}

	public void setMenu(String menu) {
		this.menu = menu;
	}

	public List<SubMenuDTO> getListMenus() {
		return listMenus;
	}

	public void setListMenus(List<SubMenuDTO> listMenus) {
		this.listMenus = listMenus;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	} 

	
}
