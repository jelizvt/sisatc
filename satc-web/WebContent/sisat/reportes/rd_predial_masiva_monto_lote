<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="resolucion_determinacion_predial_masiva" language="groovy" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="c492f10e-f47f-4b25-a199-a628bab7dea1">
	<property name="ireport.zoom" value="2.143588810000004"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["\\\\D:\\FRANPEREZ\\newtrabajo\\satc-web\\WebContent\\sisat\\reportes\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="ruta_imagen" class="java.lang.String"/>
	<parameter name="p_periodo" class="java.lang.Integer"/>
	<parameter name="p_lote_inicio" class="java.lang.Integer"/>
	<parameter name="p_lote_fin" class="java.lang.Integer"/>
	<parameter name="p_monto_minimo" class="java.math.BigDecimal"/>
	<parameter name="p_fecha_inicio" class="java.lang.String"/>
	<parameter name="p_fecha_fin" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT p.nro_docu_identidad,
p.apellidos_nombres,
a.anno_acto,
a.acto_id,
upper(ta.descripcion) tipo_acto,
upper(ta.descripcion)+' Nro '+a.nro_acto tipo_acto_nro_acto,
p.persona_id,
a.nro_cargo_notificacion,
convert(varchar(10),a.fecha_emision,103) fecha_emision,
a.nro_resolucion,
a.lote_id,
a.nro_acto,
pd.domicilio_completo desc_domicilio,
c.descripcion concepto,
c.concepto_id,
t.base_imponible,
tf.base_imponible base_imponible_fisca,
(isnull(tf.base_imponible,0)-isnull(t.base_imponible,0)) resta_base_imponible,
t.impuesto,
tf.anno_determinacion,
uit.uit,
isnull(dbo.getImpuestoTramo(t.base_imponible,$P{p_periodo},1),0) tramo_1,
isnull(dbo.getImpuestoTramo(t.base_imponible,$P{p_periodo},2),0) tramo_2,
isnull(dbo.getImpuestoTramo(t.base_imponible,$P{p_periodo},3),0) tramo_3,
isnull(dbo.getImpuestoTramo(tf.base_imponible,$P{p_periodo},1),0) tramo_1_fisca,
isnull(dbo.getImpuestoTramo(tf.base_imponible,$P{p_periodo},2),0) tramo_2_fisca,
isnull(dbo.getImpuestoTramo(tf.base_imponible,$P{p_periodo},3),0) tramo_3_fisca,
isnull(dbo.getImpuestoTramo(tf.base_imponible,$P{p_periodo},1),0)-isnull(dbo.getImpuestoTramo(t.base_imponible,$P{p_periodo},1),0)  resta_tramo1,
isnull(dbo.getImpuestoTramo(tf.base_imponible,$P{p_periodo},2),0)-isnull(dbo.getImpuestoTramo(t.base_imponible,$P{p_periodo},2),0) resta_tramo2,
isnull(dbo.getImpuestoTramo(tf.base_imponible,$P{p_periodo},3),0) -isnull(dbo.getImpuestoTramo(t.base_imponible,$P{p_periodo},3),0) resta_tramo3,
tf.determinacion_id determinacion_fisca_id,
t.determinacion_id,
isnull(tf.impuesto,0)-isnull(t.impuesto,0) diferencia
FROM dbo.cc_acto a
inner join dbo.mp_persona p on (a.persona_id=p.persona_id)
inner join dbo.mp_persona_domicilio pd on (pd.persona_id=p.persona_id and pd.estado='1')
inner join dbo.gn_concepto c on (c.concepto_id=a.concepto_id)
inner join dbo.cc_tipo_acto ta on (ta.tipo_acto_id=a.tipo_acto_id)
inner join dbo.gn_uit uit on (uit.anno_uit=$P{p_periodo})
inner join dbo.dt_determinacion tf on (tf.persona_id=p.persona_id and tf.anno_determinacion=$P{p_periodo} and tf.estado='1' and tf.fiscalizado='1' and tf.fisca_aceptada='0' and tf.fisca_cerrada='1' and tf.concepto_id=1)
left join dbo.dt_determinacion t on (t.persona_id=p.persona_id and t.anno_determinacion=$P{p_periodo} and t.estado='1' and t.fiscalizado='0' and t.concepto_id=1)
where  a.tipo_acto_id=4
	and a.monto_deuda>$P{p_monto_minimo}
	AND a.fecha_emision BETWEEN $P{p_fecha_inicio}
	AND $P{p_fecha_fin}
	AND a.lote_id BETWEEN $P{p_lote_inicio}	
	AND $P{p_lote_fin}]]>
	</queryString>
	<field name="nro_docu_identidad" class="java.lang.String"/>
