		this.djvehicularId = djvehicularId;
	}

	public int getDetalleCondicionId() {
		return this.detalleCondicionId;
	}

	public void setDetalleCondicionId(int detalleCondicionId) {
		this.detalleCondicionId = detalleCondicionId;
	}

	public Timestamp getFechaActualizacion() {
		return this.fechaActualizacion;
	}

	public void setFechaActualizacion(Timestamp fechaActualizacion) {
		this.fechaActualizacion = fechaActualizacion;
	}

	public Timestamp getFechaDocumento() {
		return this.fechaDocumento;
	}

	public void setFechaDocumento(Timestamp fechaDocumento) {
		this.fechaDocumento = fechaDocumento;
	}

	public Timestamp getFechaFinal() {
		return this.fechaFinal;
	}

	public void setFechaFinal(Timestamp fechaFinal) {
		this.fechaFinal = fechaFinal;
	}

	public Timestamp getFechaInicial() {
		return this.fechaInicial;
	}

	public void setFechaInicial(Timestamp fechaInicial) {
		this.fechaInicial = fechaInicial;
	}

	public String getGlosa() {
