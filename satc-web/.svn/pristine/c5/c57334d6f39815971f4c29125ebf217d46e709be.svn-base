                   </rich:column>
                   <rich:column >
                   	<h:outputText value="#{record.denestadoconservacion}"/>
                   </rich:column>
                   <rich:column >
                   	<h:outputText value="#{record.dentipodepreciacion}"/>
                   </rich:column>
                   <rich:column >
                   	<h:outputText value="#{record.denmuros}"/>
                   </rich:column>
                   <rich:column >
                   	<h:outputText value="#{record.dentecho}"/>
                   </rich:column>
                    <rich:column>
                   	<h:outputText value="#{record.denpisos}"/>
