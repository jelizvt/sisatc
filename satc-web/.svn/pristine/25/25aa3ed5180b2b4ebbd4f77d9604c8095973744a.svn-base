					if(findPapeleta.getEstado().equals(Constante.ESTADO_PAPELETA_DEFINITIVO)){
						listCtaCte = descargoBo.obtenerTodaDeudaPapeletas(0,findPapeleta.getPapeletaId());
					}else{
