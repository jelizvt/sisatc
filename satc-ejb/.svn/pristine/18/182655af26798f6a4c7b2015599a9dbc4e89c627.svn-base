package com.sat.sisat.cobranzacoactiva.dto;

import java.io.Serializable;

import com.sat.sisat.common.dto.BaseDTO;
import com.sat.sisat.common.util.Constante;


public class SituacionExigibilidad extends BaseDTO implements Serializable {
	private Integer loteId;
	private String tipoValor;
	private Integer cantidadValores;
	private Double totalDeudaRecibida;
	private Double totalDeudaPagada;
	private Integer cantidadValoresPendiente;
	private Integer periodoDeuda;
	private String concepto;
	
	public Integer getPeriodoDeuda() {
		return periodoDeuda;
	}
	public void setPeriodoDeuda(Integer periodoDeuda) {
		this.periodoDeuda = periodoDeuda;
	}
	public String getConcepto() {
		return concepto;
	}
	public void setConcepto(String concepto) {
		this.concepto = concepto;
	}
	public Integer getLoteId() {
		return loteId;
	}
	public void setLoteId(Integer loteId) {
		this.loteId = loteId;
	}
	public String getTipoValor() {
		return tipoValor;
	}
	public void setTipoValor(String tipoValor) {
		this.tipoValor = tipoValor;
	}
	public Integer getCantidadValores() {
		return cantidadValores;
	}
	public void setCantidadValores(Integer cantidadValores) {
		this.cantidadValores = cantidadValores;
	}
	public Double getTotalDeudaRecibida() {
		return totalDeudaRecibida;
	}
	public void setTotalDeudaRecibida(Double totalDeudaRecibida) {
		this.totalDeudaRecibida = totalDeudaRecibida;
	}
	public Double getTotalDeudaPagada() {
		return totalDeudaPagada;
	}
	public void setTotalDeudaPagada(Double totalDeudaPagada) {
		this.totalDeudaPagada = totalDeudaPagada;
	}
	public Integer getCantidadValoresPendiente() {
		return cantidadValoresPendiente;
	}
	public void setCantidadValoresPendiente(Integer cantidadValoresPendiente) {
		this.cantidadValoresPendiente = cantidadValoresPendiente;
	}
}
