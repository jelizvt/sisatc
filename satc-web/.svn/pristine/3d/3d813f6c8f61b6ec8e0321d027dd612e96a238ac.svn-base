			<h:form>
				<h:panelGrid style="padding-left:20px;">
					<rich:messages
						style="#{facesContext.maximumSeverity.ordinal==0? 'color: blue;' : 'color: red'}" />
				</h:panelGrid>
				<div style="width: 100%; text-align: center">
					<h:panelGroup id="pnlBotonesAceptarResultDJ">
