package pe.gob.sunarp.pide.controller.clientsample;

import pe.gob.sunarp.pide.controller.*;

public class ClientSample {

	public static void main(String[] args) {
	        System.out.println("***********************");
	        System.out.println("Create Web Service Client...");
	        PideService service1 = new PideService();
	        System.out.println("Create Web Service...");
	        PIDEWSService port1 = service1.getPIDEWSServicePort();
	        System.out.println("Call Web Service Operation...");
	        System.out.println("Server said: " + port1.getOficinas());
	        System.out.println("Server said: " + port1.verDetalleRPV(null,null,null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port1.buscarPJRazonSocial(null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port1.verAsiento(Long.parseLong(args[0]),Long.parseLong(args[1]),null,null,null,null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port1.listarAsientos(null,null,null,null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port1.buscarNaveAeronave(null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port1.buscarTitularidad(null,null,null,null,null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Create Web Service...");
	        PIDEWSService port2 = service1.getPIDEWSServicePort();
	        System.out.println("Call Web Service Operation...");
	        System.out.println("Server said: " + port2.getOficinas());
	        System.out.println("Server said: " + port2.verDetalleRPV(null,null,null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port2.buscarPJRazonSocial(null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port2.verAsiento(Long.parseLong(args[2]),Long.parseLong(args[3]),null,null,null,null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port2.listarAsientos(null,null,null,null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port2.buscarNaveAeronave(null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("Server said: " + port2.buscarTitularidad(null,null,null,null,null));
	        //Please input the parameters instead of 'null' for the upper method!
	
	        System.out.println("***********************");
	        System.out.println("Call Over!");
	}
}
