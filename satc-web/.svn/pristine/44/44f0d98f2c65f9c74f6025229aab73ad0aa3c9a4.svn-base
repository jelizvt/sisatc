				<h:graphicImage value="/images/modal/close.png" id="hideDeudasCobranzaCoactiva"
					styleClass="hidelink" />
				<rich:componentControl for="modalDeudasEnCobranzaCoactiva" attachTo="hideDeudasCobranzaCoactiva"
					operation="hide" event="onclick" />
			</h:panelGroup>
		</f:facet>
		<div align="center">
			<h:panelGrid columns="1" columnClasses="colModalPanel" >
				<h:outputText value="Las cuotas seleccionadas del contribuyente estan bloqueadas para su cobro. Tiene los siguientes conceptos y años en Cobranza Coactiva" />
				<ui:repeat var="iter" value="#{cuentaCorrienteManaged.lstResumenDeudasCobranzaCoactiva}">
					<h:outputText value="#{iter.conceptoDescripcion} - #{iter.anho} - #{iter.nroCuotas}" />
					<br/>
				</ui:repeat>
				<a4j:commandButton value="Aceptar" id="btnAceptarModalCobranza"/>
				<rich:componentControl for="modalDeudasEnCobranzaCoactiva" attachTo="btnAceptarModalCobranza"
					operation="hide" event="onclick" />
			</h:panelGrid>
		</div>	
		</rich:modalPanel>
	</h:form>
</ui:composition>