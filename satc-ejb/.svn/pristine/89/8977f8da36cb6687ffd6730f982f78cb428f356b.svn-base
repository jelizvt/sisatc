package com.sat.sisat.persistence.entity;

import java.io.Serializable;
import java.sql.Timestamp;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="pa_resolucion_papeleta")
public class PaResolucionPapeleta implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@Column(name="resolucion_papeleta_id")
	private Integer resolucionPapeletaId;

	@Column(name="papeleta_id")
	private Integer papeletaId;
	
	@Column(name="fecha_emision")
	private Timestamp fechaEmision;
	
	@Column(name="persona_id")
	private Integer personaId;
	
	private String observaciones;
	
	@Column(name="es_domicilio_negativa")
	private String esDomicilioNegativa;

	@Column(name="es_recepcion_negativa")
	private String esRecepcionNegativa;

	@Column(name="fecha_recepcion")
	private Timestamp fechaRecepcion;
	
	@Column(name="estado_resolucion_id")
	private Integer estadoResolucionId;
	
	private String estado;

	@Column(name="numero_resolucion")
	private String numeroResolucion;

	@Column(name="usuario_id")
	private Integer usuarioId;

	@Column(name="fecha_registro")
	private Timestamp fechaRegistro;

	@Column(name="resolucion_id")
	private Integer resolucionId;

	@Column(name="terminal")
	private String terminal;

	
    public PaResolucionPapeleta() {
    }

	public Integer getResolucionPapeletaId() {
		return this.resolucionPapeletaId;
	}

	public void setResolucionPapeletaId(Integer resolucionPapeletaId) {
		this.resolucionPapeletaId = resolucionPapeletaId;
	}

	public String getEsDomicilioNegativa() {
		return this.esDomicilioNegativa;
	}

	public void setEsDomicilioNegativa(String esDomicilioNegativa) {
		this.esDomicilioNegativa = esDomicilioNegativa;
	}

	public String getEsRecepcionNegativa() {
		return this.esRecepcionNegativa;
	}

	public void setEsRecepcionNegativa(String esRecepcionNegativa) {
		this.esRecepcionNegativa = esRecepcionNegativa;
	}

	public String getEstado() {
		return this.estado;
	}

	public void setEstado(String estado) {
		this.estado = estado;
	}

	public Integer getEstadoResolucionId() {
		return this.estadoResolucionId;
	}

	public void setEstadoResolucionId(Integer estadoResolucionId) {
		this.estadoResolucionId = estadoResolucionId;
	}

	public Timestamp getFechaEmision() {
		return this.fechaEmision;
	}

	public void setFechaEmision(Timestamp fechaEmision) {
		this.fechaEmision = fechaEmision;
	}

	public Timestamp getFechaRecepcion() {
		return this.fechaRecepcion;
	}

	public void setFechaRecepcion(Timestamp fechaRecepcion) {
		this.fechaRecepcion = fechaRecepcion;
	}

	public Timestamp getFechaRegistro() {
		return this.fechaRegistro;
	}

	public void setFechaRegistro(Timestamp fechaRegistro) {
		this.fechaRegistro = fechaRegistro;
	}

	public String getNumeroResolucion() {
		return this.numeroResolucion;
	}

	public void setNumeroResolucion(String numeroResolucion) {
		this.numeroResolucion = numeroResolucion;
	}

	public String getObservaciones() {
		return this.observaciones;
	}

	public void setObservaciones(String observaciones) {
		this.observaciones = observaciones;
	}

	public Integer getPapeletaId() {
		return this.papeletaId;
	}

	public void setPapeletaId(Integer papeletaId) {
		this.papeletaId = papeletaId;
	}

	public Integer getPersonaId() {
		return this.personaId;
	}

	public void setPersonaId(Integer personaId) {
		this.personaId = personaId;
	}

	public Integer getResolucionId() {
		return this.resolucionId;
	}

	public void setResolucionId(Integer resolucionId) {
		this.resolucionId = resolucionId;
	}

	public String getTerminal() {
		return this.terminal;
	}

	public void setTerminal(String terminal) {
		this.terminal= terminal;
	}

	public Integer getUsuarioId() {
		return this.usuarioId;
	}

	public void setUsuarioId(Integer usuarioId) {
		this.usuarioId = usuarioId;
	}

}