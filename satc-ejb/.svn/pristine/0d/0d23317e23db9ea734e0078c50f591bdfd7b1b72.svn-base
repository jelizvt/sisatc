	private String nombrePersona;
	private int codigoContribuyente;
	private BigDecimal montoCobrado;
	private BigDecimal montoIngresado;
	private String NroRecibo;
	private String NroDocumento;
	
	private String formaPago;
	private BigDecimal moneda;
	private BigDecimal tipoCambio;
	private BigDecimal montoMoneda;
	private BigDecimal montoTotal;
	private BigDecimal monto;
	
	private String obs_extorno;

 
	public String getNroRecibo() {
		return NroRecibo;
	}
	public void setNroRecibo(String nroRecibo) {
		NroRecibo = nroRecibo;
	}
	public BigDecimal getMontoCobrado() {
		return montoCobrado;
	}
	public void setMontoCobrado(BigDecimal montoCobrado) {
		this.montoCobrado = montoCobrado;
	}
	public String getTipoDocIdentidadDes() {
		return tipoDocIdentidadDes;
	}
	public void setTipoDocIdentidadDes(String tipoDocIdentidadDes) {
		this.tipoDocIdentidadDes = tipoDocIdentidadDes;
	}
	public String getNroDocIdentidad() {
		return nroDocIdentidad;
	}
	public void setNroDocIdentidad(String nroDocIdentidad) {
		this.nroDocIdentidad = nroDocIdentidad;
	}
	public String getNombrePersona() {
		return nombrePersona;
	}
	public void setNombrePersona(String nombrePersona) {
		this.nombrePersona = nombrePersona;
