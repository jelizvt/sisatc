	<field name="resta_tramo3" class="java.math.BigDecimal"/>
	<field name="determinacion_fisca_id" class="java.lang.Integer"/>
	<field name="determinacion_id" class="java.lang.Integer"/>
	<field name="diferencia" class="java.math.BigDecimal"/>
	<variable name="variable4" class="java.math.BigDecimal">
		<variableExpression><![CDATA[new Double($F{resta_tramo1}+$F{resta_tramo2}+$F{resta_tramo3})]]></variableExpression>
		<initialValueExpression><![CDATA[]]></initialValueExpression>
	</variable>
